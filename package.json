{
  "name": "kougakusai-hp",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "npm": "yarnを使ってください"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "lint:fix": "eslint --fix 'src/**/*.{ts,tsx}'",
    "lint:next": "next lint --dir src",
    "fmt": "prettier --check 'src/**/*.{ts,tsx}'",
    "fmt:fix": "prettier --write 'src/**/*.{ts,tsx}'",
    "fix": "yarn run fmt:fix && yarn run lint:fix"
  },
  "dependencies": {
    "next": "11.1.2",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@types/react": "17.0.27",
    "eslint": "7.32.0",
    "eslint-config-next": "11.1.2",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.4.1",
    "typescript": "4.4.3"
  }
}
